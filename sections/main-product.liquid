{%- liquid

assign use_variant = true
  if use_variant
    assign target = product.selected_or_first_available_variant
  else
    assign target = product
  endif

  assign compare_at_price = target.compare_at_price
  assign price = target.price | default: 1999
  assign available = target.available | default: false
-%}
<section
  id="MainProduct-{{ section.id }}"
  class="section-{{ section.id }}-padding gradient color-{{ section.settings.color_scheme }}"
  data-section="{{ section.id }}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}
  {%- if product.quantity_price_breaks_configured? -%}
    {{ 'component-volume-pricing.css' | asset_url | stylesheet_tag }}
  {%- endif -%}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top_mb }}px;
      padding-bottom: {{ section.settings.padding_bottom_mb }}px;
    }
    {%- if section.settings.enable_border -%}
      @media screen and (min-width: 990px) {
        .product__info-container {
          padding: 30px;
          border-color: {{ section.settings.border_color_info }};
          padding-top:0;
        }
      }
    {%- endif -%}
    
    .product__info-wrapper .has_boder {
      border-color: {{ section.settings.border_color_block }};
      --border-color: {{ section.settings.border_color_block }};
    }

    {%- if section.settings.enable_custom_padding -%}
      .product{
        padding: {{section.settings.padding_top_custom}}px {{section.settings.padding_right_custom}}px {{section.settings.padding_bottom_custom}}px {{section.settings.padding_left_custom}}px;
      }
    {%- endif -%}

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
    {%- if section.settings.enable_sidebar -%}
      .main-product__wrapper .product__sidebar {
        display: none;
      }
      .main-product__wrapper .product__sidebar h2 {
        margin-bottom: 24px;
      }
      @media screen and (min-width: 1360px) {
        .main-product__wrapper {
          display: flex;
          gap: 25px;
          {%- if section.settings.align_sidebar == 'left' -%}
            flex-direction: row-reverse;
          {%- endif -%}
        }
        .product.grid {
          width: calc(100% - {{ section.settings.width_sidebar }}px);
        }
        .main-product__wrapper .product__sidebar {
          display: block;
          width: {{ section.settings.width_sidebar }}px;
        }
        
      }
    {%- endif -%}
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  {%- if product.quantity_price_breaks_configured? -%}
    <script src="{{ 'show-more.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}
  <div class="page-width main-product__wrapper">
    <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-{{ section.settings.mobile_thumbnails }} grid grid--1-col {% if product.media.size > 0 %}product--mull-media{% else %}product--no-media{% endif %} color-{{ section.settings.color_scheme_content }}">
      <div class="grid__item product__media-wrapper{% if section.settings.media_position == 'right' %} medium-hide large-up-hide{% endif %}">
        {% render 'product-media-gallery', variant_images: variant_images %}
      </div>
      <div class="product__info-wrapper grid__item{% if settings.page_width > 1400 and section.settings.media_size == "small" %} product__info-wrapper--extra-padding{% endif %}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
        <product-info
          id="ProductInfo-{{ section.id }}"
          data-section="{{ section.id }}"
          data-url="{{ product.url }}"
          class="product__info-container{% if section.settings.enable_sticky_info %} product__column-sticky{% endif %}"
        >
          <div class="label__media" id="Label-product-{{ section.id }}">
            {%- if compare_at_price > price and product.quantity_price_breaks_configured? != true %}
              <span class="badge price__badge-sale badge-sale">
                {%- if settings.badge_sale_type == 'percent' -%}
                  {%-  assign current_variant = product.selected_or_first_available_variant -%}
                  {{ current_variant.compare_at_price | minus: current_variant.price | times: 100.0 | divided_by: current_variant.compare_at_price | round }}%
                {%- else -%}
                  {{ 'products.product.on_sale' | t }}
                {%- endif -%}
              </span>
            {%- endif -%}
            {%- if available == false %}
              <span class="badge price__badge-sold-out badge-sold">
                {{ 'products.product.sold_out' | t }}
              </span>
            {%- endif -%}
          </div>
          {%- assign product_form_id = 'product-form-' | append: section.id -%}
          {%- for block in section.blocks -%}
            <style>
              .block__{{ block.id }} {
                --padding-top: {{ block.settings.padding_top_mb }}px;
                --padding-bottom: {{ block.settings.padding_bottom_mb }}px
              }
              @media screen and (min-width: 750px) {
                .block__{{ block.id }} {
                  --padding-top: {{ block.settings.padding_top }}px;
                  --padding-bottom: {{ block.settings.padding_bottom }}px
                }
              }
            </style>
            <div
              class="block__product--{{ block.type }}"
            >
              {%- case block.type -%}
                {%- when '@app' -%}
                  {% render block %}
                {%- when 'vendor' -%}
                  {%- if product.vendor != '' -%}
                    <div class="block__{{ block.id }} block__info--product{% if block.settings.has_border %} has_boder{% endif %}">
                      <div
                        class="product__vendor inline-richtext"
                        {{ block.shopify_attributes }}
                      >
                        {%- if block.settings.label != blank -%}
                          <span class="label"
                          style="font-size: {{ block.settings.font_size_label }}px; color: {{ block.settings.color_label }}"      
                          >
                            {{ block.settings.label }}
                          </span>
                        {%- endif -%}
                        <a href="/collections/vendors?q={{ product.vendor}}"
                          style="font-size: {{ block.settings.font_size }}px; color: {{ block.settings.color }}"
                        >
                          {{ product.vendor}}
                        </a>
                      </div>
                    </div>
                  {%- endif -%}
                {%- when 'text' -%}
                  {%- if block.settings.text != blank -%}
                    <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      <p
                        class="product__text inline-richtext"
                        {{ block.shopify_attributes }}
                        style="font-size: {{ block.settings.font_size }}px; color: {{ block.settings.color }}"
                      >
                        {{- block.settings.text -}}
                      </p>
                    </div>
                  {%- endif -%}
                {%- when 'title' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <div class="product__title" {{ block.shopify_attributes }}>
                      <h1>{{ product.title | escape }}</h1>
                      <a href="{{ product.url }}" class="product__title">
                        <h2 class="h1">
                          {{ product.title | escape }}
                        </h2>
                      </a>
                      {%- if settings.enable_wishlist and block.settings.show_wishlist -%}
                        <wish-list class="wishlist">
                          <wish-list-icon data-productID="{{ product.id }}" class="product--icon__feature">
                            {% render 'icon' with 'wishlist' %}
                          </wish-list-icon>
                        </wish-list>
                      {%- endif -%}
                    </div>
                  </div>
                {%- when 'price' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <div class="no-js-hidden" id="price-{{ section.id }}" role="status" {{ block.shopify_attributes }}>
                      {%- if block.settings.show_sold_in -%}
                        <div class="group-pridceSold">
                      {%- endif -%}
                      {%- render 'price',
                        product: product,
                        use_variant: true,
                        show_badges: true,
                        price_class: 'price--large'
                      -%}
                      <product-last-sold-out>
                        <div class="productView-soldProduct{%unless product.available %} soldout{% endunless %}" data-sold-out-product data-item="{{ block.settings.sold_in_number }}" data-hours="{{ block.settings.sold_in_hours }}" style="display: none; color: {{block.settings.color_text_sold_in}}">
                          <div class="soldProduct__wrapper">
                            {% render 'icon' with 'fire' %}
                            <span class="text">
                                <span data-sold-out-number>22</span>
                                {{ 'products.sold_in_last.text_1' | t }}
                                <span data-sold-out-hours>12</span>
                                {{ 'products.sold_in_last.text_2' | t }}
                            </span>
                          </div>
                        </div>
                      </product-last-sold-out>
                      {%- if block.settings.show_sold_in -%}
                        </div>
                      {%- endif -%}
                    </div>
                    {%- if product.quantity_price_breaks_configured? -%}
                      <div class="volume-pricing-note" id="Volume-Note-{{ section.id }}">
                        <span>{{ 'products.product.volume_pricing.note' | t }}</span>
                      </div>
                    {%- endif -%}
                    <div class="product__tax caption rte">
                      {%- if cart.taxes_included -%}
                        {{ 'products.product.include_taxes' | t }}
                      {%- endif -%}
                      {%- if shop.shipping_policy.body != blank -%}
                        {{ 'products.product.shipping_policy_html' | t: link: shop.shipping_policy.url }}
                      {%- endif -%}
                    </div>
                    <div {{ block.shopify_attributes }}>
                      {%- assign product_form_installment_id = 'product-form-installment-' | append: section.id -%}
                      {%- form 'product', product, id: product_form_installment_id, class: 'installment caption-large' -%}
                        <input type="hidden" name="id" value="{{ product.selected_or_first_available_variant.id }}">
                        {{ form | payment_terms }}
                      {%- endform -%}
                    </div>
                  </div>
                {%- when 'inventory' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <p
                      class="product__inventory no-js-hidden{% if product.selected_or_first_available_variant.inventory_management != 'shopify' %} visibility-hidden{% endif %}"
                      {{ block.shopify_attributes }}
                      id="Inventory-{{ section.id }}"
                      role="status"
                    >
                      {%- if block.settings.label_inven != blank -%}
                        <span class="label"
                        style="font-size: {{ block.settings.font_size_label_inven }}px; color: {{ block.settings.color_label_inven }}"      
                        >
                          {{ block.settings.label_inven }}: 
                        </span>
                      {%- endif -%}
                      {%- if product.selected_or_first_available_variant.inventory_management == 'shopify' -%}
                        {%- if product.selected_or_first_available_variant.inventory_quantity > 0 -%}
                          {%- if product.selected_or_first_available_variant.inventory_quantity
                              <= block.settings.inventory_threshold
                          -%}
                            {%- if block.settings.show_inventory_quantity -%}
                              {{-
                                'products.product.inventory_low_stock_show_count'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              -}}
                            {%- else -%}
                              {{- 'products.product.inventory_low_stock' | t -}}
                            {%- endif -%}
                          {%- else -%}
                            {%- if block.settings.show_inventory_quantity -%}
                              {{-
                                'products.product.inventory_in_stock_show_count'
                                | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                              -}}
                            {%- else -%}
                              {{- 'products.product.inventory_in_stock' | t -}}
                            {%- endif -%}
                          {%- endif -%}
                        {%- else -%}
                          {%- if product.selected_or_first_available_variant.inventory_policy == 'continue' -%}
                            {{- 'products.product.inventory_out_of_stock_continue_selling' | t -}}
                          {%- else -%}
                            {{- 'products.product.inventory_out_of_stock' | t -}}
                          {%- endif -%}
                        {%- endif -%}
                      {%- endif -%}
                    </p>
                  </div>
                {%- when 'description' -%}
                  {%- if product.description != blank -%}
                    <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      <div class="product__description rte quick-add-hidden" {{ block.shopify_attributes }}>
                        {{ product.description }}
                      </div>
                    </div>
                  {%- endif -%}
                {%- when 'sku' -%}
                  {%- if product.selected_or_first_available_variant.sku != blank -%}
                    <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      <p
                        class="product__sku no-js-hidden{% if product.selected_or_first_available_variant.sku.size == 0 %} visibility-hidden{% endif %}"
                        id="Sku-{{ section.id }}"
                        role="status"
                        {{ block.shopify_attributes }}
                      >
                        {%- if block.settings.label != blank -%}
                          <span class="product-info__label">{{ block.settings.label }}</span>
                        {%- endif -%}
                        {{- product.selected_or_first_available_variant.sku -}}
                      </p>
                    </div>
                  {%- endif -%}
                {%- when 'property' -%}
                  {%- if product.selected_or_first_available_variant.sku != blank or product.collections.size > 1 -%}
                    <div class="block__info--product d-flex wrap a-i--center j-c--space{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      {%- if product.selected_or_first_available_variant.sku != blank and block.settings.enable_sku -%}
                        <p
                          class="product__sku no-js-hidden{% if product.selected_or_first_available_variant.sku.size == 0 %} visibility-hidden{% endif %}"
                          id="Sku-{{ section.id }}"
                          role="status"
                          {{ block.shopify_attributes }}
                        >
                          {%- if block.settings.label_sku != blank -%}
                            <span class="product-info__label">{{ block.settings.label_sku }}</span>
                          {%- endif -%}
                          {{- product.selected_or_first_available_variant.sku -}}
                        </p>
                      {%- endif -%}
                      {%- if block.settings.enable_share -%}
                        {% assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin %}
                        {% render 'share-button', block: block, share_link: share_url, is_prduct: true %}
                      {%- endif -%}
                    </div>
                  {%- endif -%}
                {%- when 'categories' -%}
                  {% if product.collections.size > 1  %}
                    <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      <div
                        class="product__categories"
                        {{ block.shopify_attributes }}
                        style="--text_color:{{block.settings.text_color}}"
                      >
                        {%- if block.settings.label != blank -%}
                          <span class="product-info__label">{{ block.settings.label }}</span>
                        {%- endif -%}
                        <ul>
                          {% for collection in product.collections %}
                            {%- unless collection.title == 'all' or collection.title == 'All' -%}
                              <li>
                                <a href="{{ collection.url }}">{{ collection.title }}{% unless forloop.last %},{% endunless %}</a>
                              </li>
                            {%- endunless -%}
                          {% endfor %}
                        </ul>
                      </div>
                    </div>
                  {%- endif -%}
                {%- when 'tags' -%}
                  {% if product.tags.size > 1  %}
                    <div class="block__info--product {% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px
                      "
                    >
                      <div
                        class="product__tags"
                        {{ block.shopify_attributes }}
                        style="--text_color:{{block.settings.text_color}}"
                      >
                        {%- if block.settings.label != blank -%}
                          <span class="product-info__label">{{ block.settings.label }}</span>
                        {%- endif -%}
                        <ul>
                          {% for tag in product.tags %}
                            <li>
                              <span>{{ tag }}{% unless forloop.last %},{% endunless %}</span>
                            </li>
                          {% endfor %}
                        </ul>
                      </div>
                    </div>
                  {%- endif -%}
                  {%- when 'customer_viewing' -%}
                    <div class="block__info--product productView-moreItem"
                      style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;
                    "
                    >
                    <product-customer-viewing>
                      <div class="productView-ViewingProduct" data-customer-view="{{block.settings.customer_viewing_number }}" data-customer-view-time="{{ block.settings.customer_viewing_time }}">
                        {% render 'icon' with 'eye' %}
                        <span class="text">{{ 'products.customer_viewing.general.text' | t: number : '97' }}</span>
                      </div>
                    </product-customer-viewing>
                {%- when 'countdown' -%}
                  {%- if block.settings.countdown_type == 'all' -%}
                    {%- assign timer =  block.settings.date -%}
                  {%- else -%}
                    {%- assign timer = product.metafields.custom.countdown -%}
                  {%- endif -%}
                  {%- if timer != blank -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;
                      --color_border: {{ block.settings.color_border }};
                    "
                  >
                    <div
                      class="product__countdown a-i--center layout-{{block.settings.layout_count}}"
                      style="
                        {% if block.settings.bg != blank %}
                          background-image: url({{ block.settings.bg | img_url: 'master' }});
                          padding: 5px 15px;
                        {% endif %}
                        background-color: {{ block.settings.bg_color }};
                        color: {{ block.settings.color }};
                        font-size: {{ block.settings.font_size }}px;
                      "
                      {{ block.shopify_attributes }}
                    >
                    <div class="count_label">

                      {%- if block.settings.icon_countdown != blank -%}
                        <img
                          src="{{ block.settings.icon_countdown | image_url }}"
                          {% if block.settings.icon_countdown.alt != blank %}
                            alt="{{ block.settings.icon_countdown.alt | escape }}"
                          {% else %}
                            role="presentation"
                          {% endif %}
                          height="auto"
                          width="auto"
                          loading="lazy"
                        >
                      {%- endif -%}
                      {%- if block.settings.label != blank  -%}
                        <span class="product-info__label"
                        style="font-size:{{block.settings.font_size_label}}px; color: {{block.settings.color_label}}"
                        
                        >{{ block.settings.label }}</span>
                      {%- endif -%}
                    </div>
                      
                      {%- if timer != blank -%}
                        {%- if block.settings.layout_count == "style_1" -%}
                          <countdown-time-grid class="countdown__time countdown__style_1" timer="{{ timer }}" hasHTML="true">
                            <div class="show_countdown"></div>
                          </countdown-time-grid>
                        {%- else -%}
                          <countdown-time class="countdown__time" timer="{{ timer }}">
                            <span class="show_countdown"></span>
                          </countdown-time>
                        {%- endif -%}
                      {%- endif -%}
                    </div>
                  </div>
                {%- endif -%}
                {%- when 'icon-with-text-sidebar' -%}
                  <div class="block__info--product icon-with-text-sidebar{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;
                      --color-background:{{block.settings.backgroud}};
                      --color-text:{{block.settings.text_color}};
                    "
                  >
                    {% render 'icon-with-text', block: block %} 
                  </div>
                {% when 'trust_badge'%}
                  <div class="trust-badge block__info--product" {{ block.shopify_attributes }}
                  style="
                    --padding-top: {{ block.settings.padding_top }}px;
                    --padding-bottom: {{ block.settings.padding_bottom }}px
                  "
                  >
                    <ul class="list-trust__badge clearfix">
                      {%- for i in (1..5) -%}
                      {% capture image %}truct_image_{{ i }}{% endcapture %}
                      {%- if block.settings[image] != blank -%}
                        <li>
                            {%- assign img =  block.settings[image] -%}
                              <img
                                src="{{ img | img_url: 'master' }}"
                                {% if img.alt != blank %}
                                  alt="{{ img.alt | escape }}"
                                {% else %}
                                  role="presentation"
                                {% endif %}
                                height="auto"
                                width="auto"
                                loading="lazy"
                              >
                        </li>
                      {%- endif -%}
                    {%- endfor -%}
                    </ul>
                  </div>
                {%- when 'collapsible_tab' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <div class="product__accordion accordion quick-add-hidden" {{ block.shopify_attributes }}>
                      <details id="Details-{{ block.id }}-{{ section.id }}">
                        <summary>
                          <div class="summary__title">
                            {% render 'icon-accordion', icon: block.settings.icon %}
                            <h2 class="h4 accordion__title inline-richtext">
                              {{ block.settings.heading | default: block.settings.page.title }}
                            </h2>
                          </div>
                          {% render 'icon-caret' %}
                        </summary>
                        <div class="accordion__content rte" id="ProductAccordion-{{ block.id }}-{{ section.id }}">
                          {{ block.settings.content }}
                          {{ block.settings.page.content }}
                        </div>
                      </details>
                    </div>
                  </div>
                {%- when 'popup' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <modal-opener
                      class="product-popup-modal__opener no-js-hidden quick-add-hidden"
                      data-modal="#PopupModal-{{ block.id }}"
                      {{ block.shopify_attributes }}
                    >
                      <button
                        id="ProductPopup-{{ block.id }}"
                        class="product-popup-modal__button link"
                        type="button"
                        aria-haspopup="dialog"
                      >
                        {{ block.settings.text | default: block.settings.page.title }}
                      </button>
                    </modal-opener>
                    <a href="{{ block.settings.page.url }}" class="product-popup-modal__button link no-js">
                      {{- block.settings.text -}}
                    </a>
                  </div>
                {%- when 'share' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    {% assign share_url = product.selected_variant.url | default: product.url | prepend: request.origin %}
                    {% render 'share-button', block: block, share_link: share_url, is_prduct: true %}
                  </div>
                {%- when 'pickup' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    <div class="local__pickup--block">
                      {{ 'component-pickup-availability.css' | asset_url | stylesheet_tag }}

                      {%- assign pick_up_availabilities = product.selected_or_first_available_variant.store_availabilities
                        | where: 'pick_up_enabled', true
                      -%}

                      <pickup-availability
                        class="product__pickup-availabilities no-js-hidden quick-add-hidden"
                        {% if product.selected_or_first_available_variant.available and pick_up_availabilities.size > 0 %}
                          available
                        {% endif %}
                        data-root-url="{{ routes.root_url }}"
                        data-variant-id="{{ product.selected_or_first_available_variant.id }}"
                        data-has-only-default-variant="{{ product.has_only_default_variant }}"
                        data-product-page-color-scheme="gradient color-{{ section.settings.color_scheme }}"
                      >
                        <template>
                          <pickup-availability-preview class="pickup-availability-preview">
                            {% render 'icon-unavailable' %}
                            <div class="pickup-availability-info">
                              <p class="caption-large">{{ 'products.product.pickup_availability.unavailable' | t }}</p>
                              <button class="pickup-availability-button link link--text underlined-link">
                                {{ 'products.product.pickup_availability.refresh' | t }}
                              </button>
                            </div>
                          </pickup-availability-preview>
                        </template>
                      </pickup-availability>

                      <script src="{{ 'pickup-availability.js' | asset_url }}" defer="defer"></script>
                    </div>
                  </div>
                {%- when 'variant_picker' -%}
                  {%- unless product.has_only_default_variant -%}
                    <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                      style="
                        --padding-top: {{ block.settings.padding_top }}px;
                        --padding-bottom: {{ block.settings.padding_bottom }}px;
                      "
                    >
                      {% render 'product-variant-picker',
                        product: product,
                        block: block,
                        product_form_id: product_form_id
                      %}
                    </div>
                  {%- endunless -%}
                {%- when 'buy_buttons' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    {%- render 'buy-buttons',
                      block: block,
                      product: product,
                      product_form_id: product_form_id,
                      section_id: section.id,
                      show_pickup_availability: true
                    -%}
                  </div>
                {%- when 'rating' -%}
                  <div class="block__info--product{% if block.settings.has_border %} has_boder{% endif %}"
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px
                    "
                  >
                    {%- if product.metafields.judgeme.badge != blank -%}
                      {% liquid
                        assign rating_decimal = 0
                        assign decimal = product.metafields.reviews.rating.value.rating | modulo: 1
                        if decimal >= 0.3 and decimal <= 0.7
                          assign rating_decimal = 0.5
                        elsif decimal > 0.7
                          assign rating_decimal = 1
                        endif
                        assign rating_floor = product.metafields.reviews.rating.value.rating | floor
                      %}
                      <div
                        class="rating jdgm-rev-widg__summary-stars"
                        role="img"
                        aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}"
                      >
                        {{ product.metafields.judgeme.badge }}
                        <p class="rating-text caption">
                          <span>
                            {{- product.metafields.reviews.rating.value }} 
                            {% comment %} /{{ product.metafields.reviews.rating.value.scale_max -}} {% endcomment %}
                          </span>
                        </p>
                      </div>
                    {%- else -%}
                      <div
                        class="rating jdgm-rev-widg__summary-stars"
                        role="img"
                        aria-label="{{ 'accessibility.star_reviews_info' | t: rating_value: product.metafields.reviews.rating.value, rating_max: product.metafields.reviews.rating.value.scale_max }}"
                      >
                        {%- for i in (1..5) -%}
                          <span class="jdgm-star jdgm--off"></span>
                        {%- endfor -%}
                        <p class="rating-text caption">
                          <span>
                            0.0
                          </span>
                        </p>
                      </div>
                    {%- endif -%}
                    <span class="separation"></span>
                    <span class="inventory_in_stock_cst">
                      {{-
                        'products.product.inventory_in_stock_show_count'
                        | t: quantity: product.selected_or_first_available_variant.inventory_quantity
                      -}}
                    </span>
                  </div>
                
                {%- when 'short_description' -%}
                  {%- if block.settings.short_option_content == 'all' -%}
                    {% assign short_description = block.settings.short_content %}
                  {%- else -%}
                    {% assign short_description = product.metafields.custom.short_description %}
                  {%- endif -%}
                  {%- if short_description != blank -%}
                    <div class="short_description" 
                    style="
                      --padding-top: {{ block.settings.padding_top }}px;
                      --padding-bottom: {{ block.settings.padding_bottom }}px;
                      --font-size: {{ block.settings.font_size }}px; 
                      --color: {{ block.settings.color }}
                    ">
                      {{short_description}}
                    </div>
                  {%- endif -%}
              {%- endcase -%}
            </div>
          {%- endfor -%}
        </product-info>
      </div>
      {%- if section.settings.media_position == 'right' -%}
        {% comment %} Duplicate gallery to display after product content on tablet/desktop breakpoint {% endcomment %}
        <div class="grid__item product__media-wrapper small-hide">
          {% render 'product-media-gallery', variant_images: variant_images, is_duplicate: true %}
        </div>
      {%- endif -%}
    </div>
    {%- if section.settings.enable_sidebar -%}
      <div class="product__sidebar quick-add-hidden">
        {%- render 'sidebar-product', horizontal_class: true -%}
      </div>
    {%- endif -%}

    {% render 'product-media-modal', variant_images: variant_images %}

    {% assign popups = section.blocks | where: 'type', 'popup' %}
    {%- for block in popups -%}
      <modal-dialog id="PopupModal-{{ block.id }}" class="product-popup-modal" {{ block.shopify_attributes }}>
        <div
          role="dialog"
          aria-label="{{ block.settings.text }}"
          aria-modal="true"
          class="product-popup-modal__content"
          tabindex="-1"
        >
          <button
            id="ModalClose-{{ block.id }}"
            type="button"
            class="product-popup-modal__toggle"
            aria-label="{{ 'accessibility.close' | t }}"
          >
            {% render 'icon-close' %}
          </button>
          <div class="product-popup-modal__content-info">
            <h1 class="h2">{{ block.settings.page.title }}</h1>
            {{ block.settings.page.content }}
          </div>
        </div>
      </modal-dialog>
    {%- endfor -%}
    {% if settings.show_size_chart %}
      <modal-dialog id="PopupModal-sizeChart" class="product-popup-modal">
          <div
              role="dialog"
              aria-label="Size Chart"
              aria-modal="true"
              class="product-popup-modal__content"
              tabindex="-1"
          >
              <button
              id="ModalClose-sizeChart"
              type="button"
              class="product-popup-modal__toggle"
              aria-label="{{ 'accessibility.close' | t }}"
              >
              {% render 'icon-close' %}
              </button>
              <div class="product-popup-modal__content-info">
                {%- if settings.title_size_chart != blank -%}
                  <h1 class="h2">{{ settings.title_size_chart }}</h1>
                {%- endif %}
                <div class="popup-modal__content">
                  {%- if settings.size_chart_type == 'metafield' -%}
                    {% assign image = product.metafields.custom.size_chart | file_url %}
                      {% if image != blank %}
                        <img
                          src="{{ image}}"
                          {% if image.alt != blank %}
                            alt="{{ img.alt | escape }}"
                          {% else %}
                            role="presentation"
                          {% endif %}
                          height="auto"
                          width="auto"
                          loading="lazy"
                        >
                      {% endif %}
                  {%- else -%}
                    {% assign image = settings.size_chart_image %}
                      {% if image != blank %}
                        <img
                          src="{{ image | img_url: 'master' }}"
                          {% if image.alt != blank %}
                            alt="{{ img.alt | escape }}"
                          {% else %}
                            role="presentation"
                          {% endif %}
                          height="auto"
                          width="auto"
                          loading="lazy"
                        >
                      {% endif %}
                  {%- endif -%}
                </div>
              </div>
          </div>
      </modal-dialog>
    {% endif %}
    {%- if product.media.size > 0 -%}
      <script src="{{ 'product-modal.js' | asset_url }}" defer="defer"></script>
      <script src="{{ 'media-gallery.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {%- if first_3d_model -%}
      <script type="application/json" id="ProductJSON-{{ product.id }}">
        {{ product.media | where: 'media_type', 'model' | json }}
      </script>
      <script src="{{ 'product-model.js' | asset_url }}" defer></script>
    {%- endif -%}

    <script>
      document.addEventListener('DOMContentLoaded', function () {
        function isIE() {
          const ua = window.navigator.userAgent;
          const msie = ua.indexOf('MSIE ');
          const trident = ua.indexOf('Trident/');

          return msie > 0 || trident > 0;
        }

        if (!isIE()) return;
        const hiddenInput = document.querySelector('#{{ product_form_id }} input[name="id"]');
        const noScriptInputWrapper = document.createElement('div');
        const variantSwitcher =
          document.querySelector('variant-radios[data-section="{{ section.id }}"]') ||
          document.querySelector('variant-selects[data-section="{{ section.id }}"]');
        noScriptInputWrapper.innerHTML = document.querySelector(
          '.product-form__noscript-wrapper-{{ section.id }}'
        ).textContent;
        variantSwitcher.outerHTML = noScriptInputWrapper.outerHTML;

        document.querySelector('#Variants-{{ section.id }}').addEventListener('change', function (event) {
          hiddenInput.value = event.currentTarget.value;
        });
      });
    </script>

    {%- liquid
      if product.selected_or_first_available_variant.featured_media
        assign seo_media = product.selected_or_first_available_variant.featured_media
      else
        assign seo_media = product.featured_media
      endif
    -%}

    <script type="application/ld+json">
      {
        "@context": "http://schema.org/",
        "@type": "Product",
        "name": {{ product.title | json }},
        "url": {{ request.origin | append: product.url | json }},
        {% if seo_media -%}
          "image": [
            {{ seo_media | image_url: width: 1920 | prepend: "https:" | json }}
          ],
        {%- endif %}
        "description": {{ product.description | strip_html | json }},
        {% if product.selected_or_first_available_variant.sku != blank -%}
          "sku": {{ product.selected_or_first_available_variant.sku | json }},
        {%- endif %}
        "brand": {
          "@type": "Brand",
          "name": {{ product.vendor | json }}
        },
        "offers": [
          {%- for variant in product.variants -%}
            {
              "@type" : "Offer",
              {%- if variant.sku != blank -%}
                "sku": {{ variant.sku | json }},
              {%- endif -%}
              {%- if variant.barcode.size == 12 -%}
                "gtin12": {{ variant.barcode }},
              {%- endif -%}
              {%- if variant.barcode.size == 13 -%}
                "gtin13": {{ variant.barcode }},
              {%- endif -%}
              {%- if variant.barcode.size == 14 -%}
                "gtin14": {{ variant.barcode }},
              {%- endif -%}
              "availability" : "http://schema.org/{% if variant.available %}InStock{% else %}OutOfStock{% endif %}",
              "price" : {{ variant.price | divided_by: 100.00 | json }},
              "priceCurrency" : {{ cart.currency.iso_code | json }},
              "url" : {{ request.origin | append: variant.url | json }}
            }{% unless forloop.last %},{% endunless %}
          {%- endfor -%}
        ]
      }
    </script>
  </div>
</section>

